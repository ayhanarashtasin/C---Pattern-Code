//Question is Print this pattern
// A 
// B C 
// D E F 
// G H I J 

#include<iostream>
#include<cmath>
using namespace std;
int main()
{
  int n;
  cout<<"Enter your desired number : ";
  cin>>n;
  char element = 'A';
  for(int i=0; i<n; i++){
    for(int j=0; j<=i; j++){
      cout<<element<<" ";
      element++;
    }
    cout<<endl;
  }
  return 0;
}
